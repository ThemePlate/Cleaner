{
	"name": "themeplate/cleaner",
	"description": "ThemePlate markup cleaner",
	"license": "GPL-3.0-only",
	"authors": [
		{
			"name": "Gene Alyson Fortunado Torcende",
			"email": "genealyson.torcende@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		},
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"require": {
		"php": "^7.4|^8.0"
	},
	"suggest": {
		"themeplate/navwalker": "^1.0"
	},
	"require-dev": {
		"themeplate/tester": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"ThemePlate\\Cleaner\\": "src"
		},
		"files": [
			"Cleaner.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests"
		}
	}
}
